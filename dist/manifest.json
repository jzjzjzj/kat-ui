{
  "name": "KAT UI - Kickass Torrents Special",
  "short_name": "KAT UI",
  "description": "Improved user interface of KickassTorrents (kat).",
  "version": "0.2.0",
  "icons": {
    "16": "img/16.png",
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "js.js"
      ],
      "css": [
        "css.css"
      ],
      "matches": [
        "https://kickasstorrents.im/*",
        "http://kickasstorrents.im/*",
        "https://kickass.to/*",
        "http://kickass.to/*"
      ]
    }
  ],
  "web_accessible_resources": [
    "highlight.js"
  ]
}
